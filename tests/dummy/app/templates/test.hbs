<div>
  <form id="profileSignUpForm" class="LoginContainer">
    <div class="sixteen columns 30in">
      <h1>Welcome to CherryTree.</h1><br>
      <h3>The Online Farmers' Market.</h3>
    </div>
    <div class="form-group">
      <label for="input-email">Email</label>
      {{input type="text" id="input-email" class="form-control" placeholder="E-mail" autocomplete="email" value=model.email}}
      <small id="emailHelp" class="form-text text-muted">We'll never share your email</small>
    </div>
    <div class="form-group">
      <label for="input-user">Username</label>
      {{input type="text" id="input-user" class="form-control" placeholder="Username" autocomplete="username" value=model.username}}
    </div>
    <div class="form-group">
      <label for="pass">Password</label>
      {{input type="password" id="pass" class="form-control" placeholder="Password" autocomplete="current-password" value=model.password }}
    </div>
    <br>
    {{button-primary clickHandler=(action 'onClick') text="Sign Up" id="subbutton"}}
  </form>
</div>
{{outlet}}
